    pod 'FBSDKShareKit'
    pod 'ObjectMapper'
    pod 'KeychainSwift'
    pod 'UICountingLabel'
    pod 'R.swift'
    pod 'LTMorphingLabel'
end

target 'HoldemHands' do
    pods
end

